package com.openbet.socailbetting.webapp;

import java.util.List;

public class BetSettlementDTO {
	private Double amtPayout = 0.0;
	private List<String> payoutReceiverIds;
	private List<BetOpponentDTO> opponents;
	private Boolean isSettled;
	private boolean isTie;
	
	public Double getAmtPayout() {
		return amtPayout;
	}
	public void setAmtPayout(Double amtPayout) {
		this.amtPayout = amtPayout;
	}
	
	public List<String> getPayoutReceiverIds() {
		return payoutReceiverIds;
	}
	public void setPayoutReceiverIds(List<String> payoutReceiverIds) {
		this.payoutReceiverIds = payoutReceiverIds;
	}
	public List<BetOpponentDTO> getOpponents() {
		return opponents;
	}
	public void setOpponents(List<BetOpponentDTO> opponents) {
		this.opponents = opponents;
	}
	public Boolean getIsSettled() {
		return isSettled;
	}
	public void setIsSettled(Boolean isSettled) {
		this.isSettled = isSettled;
	}
	public boolean isTie() {
		return isTie;
	}
	public void setTie(boolean isTie) {
		this.isTie = isTie;
	}
	
}
